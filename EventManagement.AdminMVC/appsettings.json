{
  "JWTConfiguration": {
    "Secret": "ABABABABABBABABABABA",
    "ExpirationInMinutes": 5
  },
  "DirectoriesPath": {
    "EventFilesPath": "C:\\Users\\nikusha\\source\\repos\\ovashvili\\EventManagement\\EventManagement.MVC\\EventManagement.Shared\\SharedImages"
  },
  "HealthChecksUI": {
    "HealthChecks": [
      {
        "Name": "Health Checks Dashboard",
        "Uri": "/healthcheck"
      }
    ],
    "EvaluationTimeInSeconds": 10
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=EventManagementDB;Trusted_Connection=True;",
    "Redis": "localhost:6379",
    "EventDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=EventManagement.AdminMVC;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId}{NewLine}{Level:u3}{NewLine}{Message:lj}{Exception}{NewLine}{NewLine}{NewLine}",
          "theme": "Serilog.Sinks.SystemConsole.Themes.SystemConsoleTheme::Grayscale, Serilog.Sinks.Console"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId}{NewLine}{Level:u3}{NewLine}{Message:lj}{Exception}{NewLine}{NewLine}{NewLine}"
        }
      }
    ]
  }
}
